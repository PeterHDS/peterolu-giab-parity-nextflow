{
  "Version": "2012-10-17",
  "Statement": [
    { "Sid": "S3BucketWideList",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::portable-precision-peterhds-eu2" },
    { "Sid": "S3ReadAssets",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::portable-precision-peterhds-eu2/assets/*" },
    { "Sid": "S3WriteWorkAndResults",
      "Effect": "Allow",
      "Action": ["s3:GetObject","s3:PutObject","s3:DeleteObject","s3:AbortMultipartUpload","s3:ListBucketMultipartUploads"],
      "Resource": "arn:aws:s3:::portable-precision-peterhds-eu2/*" },
    { "Sid": "LogsWrite",
      "Effect": "Allow",
      "Action": ["logs:CreateLogStream","logs:PutLogEvents"],
      "Resource": "*" }
  ]
}
